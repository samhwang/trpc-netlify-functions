{
  "name": "@samhwang/trpc-netlify-functions",
  "version": "0.1.0",
  "description": "An adapter to run tRPC Server on Netlify Functions",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "lint": "eslint --cache .",
    "lint:fix": "npm run lint --fix",
    "prettier": "prettier --cache --write .",
    "format": "npm run lint:fix && npm run prettier && prisma format",
    "typecheck": "tsc",
    "release": "pnpm run build && changeset publish"
  },
  "keywords": ["trpc", "netlify", "typescript"],
  "author": "Sam Huynh",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/samhwang/trpc-netlify-functions.git"
  },
  "peerDependencies": {
    "@netlify/functions": "^1.3.0",
    "@trpc/server": "^10.14.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "tsup": "^6.5.0"
  }
}
